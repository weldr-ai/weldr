{
  "name": "marketing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev --turbo -p 3000",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env next start",
    "check-types": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@integramind/ui": "workspace:*",
    "@t3-oss/env-nextjs": "^0.9.2",
    "framer-motion": "^11.2.10",
    "lucide-react": "^0.363.0",
    "mini-svg-data-uri": "^1.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@integramind/db": "workspace:*",
    "@integramind/eslint-config": "workspace:*",
    "@integramind/prettier-config": "workspace:*",
    "@integramind/tailwind-config": "workspace:*",
    "@integramind/tsconfig": "workspace:*",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.18",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "jiti": "^1.21.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@integramind/eslint-config/base",
      "@integramind/eslint-config/nextjs",
      "@integramind/eslint-config/react"
    ]
  },
  "prettier": "@integramind/prettier-config"
}
