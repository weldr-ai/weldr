{
  "name": "my-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --platform=node --packages=external --bundle --minify --format=esm --outdir=dist",
    "start": "node dist/index.js",
    "check-types": "tsc --noEmit",
    "check": "biome check --diagnostic-level=warn .",
    "check:fix": "biome check --diagnostic-level=warn --write ."
  },
  "dependencies": {
    "@hono/node-server": "^1.14.3",
    "@hono/zod-openapi": "^0.19.8",
    "@orpc/openapi": "^1.4.0",
    "@orpc/zod": "^1.4.0",
    "hono": "^4.7.11",
    "hono-pino": "^0.8.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.15.30",
    "esbuild": "^0.25.5",
    "tsx": "^4.20.0",
    "typescript": "^5.8.3"
  }
}
