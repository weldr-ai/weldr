{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev -p 3001",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@integramind/auth": "workspace:*",
    "@integramind/ui": "workspace:*",
    "@paralleldrive/cuid2": "^2.2.2",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "lucide-react": "^0.363.0",
    "next": "^14.2.3",
    "next-auth": "5.0.0-beta.17",
    "next-themes": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.2",
    "reactflow": "^11.11.3",
    "slate": "^0.103.0",
    "slate-react": "^0.102.0",
    "zod": "link:@hookform/resolvers/zod",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@integramind/eslint-config": "workspace:*",
    "@integramind/prettier-config": "workspace:*",
    "@integramind/tailwind-config": "workspace:*",
    "@integramind/tsconfig": "workspace:*",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.18",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "jiti": "^1.21.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@integramind/eslint-config/base",
      "@integramind/eslint-config/nextjs",
      "@integramind/eslint-config/react"
    ]
  },
  "prettier": "@integramind/prettier-config"
}
