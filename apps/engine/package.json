{
  "name": "@integramind/engine",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "node esbuild.config.js",
    "dev": "pnpm with-env tsx --watch src/index.ts",
    "start": "npx --yes listhen --open dist/index.js --port 3000",
    "preview": "pnpm run build && npx --yes listhen --open dist/index.js --port 3000",
    "clean": "git clean -xdf .turbo node_modules dist src/temp",
    "check-types": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "h3": "^1.12.0",
    "handlebars": "^4.7.8",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.2.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.2",
    "esbuild-plugin-copy": "^2.1.1",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.12.3"
}
