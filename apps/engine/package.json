{
  "name": "@integramind/engine",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsx --watch --no-node-snapshot src/index.ts",
    "clean": "git clean -xdf .turbo node_modules dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "check-types": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@integramind/api": "workspace:*",
    "@integramind/auth": "workspace:*",
    "@integramind/db": "workspace:*",
    "@trpc/server": "11.0.0-rc.441",
    "express": "^4.19.2",
    "isolated-vm": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@integramind/eslint-config": "workspace:*",
    "@integramind/prettier-config": "workspace:*",
    "@integramind/tsconfig": "workspace:*",
    "@types/express": "^4.17.21",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@integramind/eslint-config/express"
    ]
  },
  "prettier": "@integramind/prettier-config"
}
