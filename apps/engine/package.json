{
  "name": "engine",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsx src/index.ts",
    "dev": "tsx --watch --no-node-snapshot src/index.ts",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "check-types": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@integramind/api": "workspace:*",
    "@integramind/auth": "workspace:*",
    "@integramind/db": "workspace:*",
    "@trpc/server": "11.0.0-rc.441",
    "express": "^4.19.2",
    "isolated-vm": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@integramind/eslint-config": "workspace:*",
    "@integramind/prettier-config": "workspace:*",
    "@integramind/tsconfig": "workspace:*",
    "@types/express": "^4.17.21",
    "tsx": "^4.11.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@integramind/eslint-config/base"
    ]
  },
  "prettier": "@integramind/prettier-config"
}
