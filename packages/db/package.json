{
  "name": "@integramind/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema/index.ts",
    "./utils": "./src/utils.ts"
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules dist",
    "check": "pnpm with-env drizzle-kit check --config src/config.ts",
    "generate": "pnpm with-env drizzle-kit generate --config src/config.ts",
    "migrate": "pnpm with-env drizzle-kit migrate --config src/config.ts",
    "migrate:drop": "pnpm with-env drizzle-kit drop --config src/config.ts",
    "pull": "pnpm with-env drizzle-kit introspect --config src/config.ts",
    "push": "pnpm with-env drizzle-kit push --config src/config.ts",
    "studio": "pnpm with-env drizzle-kit studio --config src/config.ts",
    "up": "pnpm with-env drizzle-kit up --config src/config.ts",
    "check-types": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@integramind/shared": "workspace:*",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.6.1",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@integramind/tsconfig": "workspace:*",
    "@types/pg": "^8.11.10",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.28.1",
    "typescript": "^5.7.2"
  }
}
