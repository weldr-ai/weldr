{
  "name": "@weldr/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema/index.ts",
    "./utils": "./src/utils.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules dist",
    "check": "pnpm with-env drizzle-kit check",
    "generate": "pnpm with-env drizzle-kit generate",
    "migrate": "pnpm with-env drizzle-kit migrate",
    "migrate:drop": "pnpm with-env drizzle-kit drop",
    "pull": "pnpm with-env drizzle-kit introspect",
    "push": "pnpm with-env drizzle-kit push",
    "studio": "pnpm with-env drizzle-kit studio",
    "seed": "pnpm with-env tsx src/seed.ts",
    "up": "pnpm with-env drizzle-kit up",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@weldr/shared": "workspace:*",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.6.1",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@weldr/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.4",
    "drizzle-kit": "^0.30.6",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
