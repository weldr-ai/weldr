          "name": "ToggleGroup",
          "description": "A wrapper component that groups multiple toggle items together. It provides context for styling and behavior of child ToggleGroupItem components. Built on top of Radix UI's ToggleGroup primitive.",
          "subtype": "reusable",
          "rendersOn": "client",
          "properties": {
            "type": "object",
            "properties": {
              "variant": {
                "type": "string",
                "description": "Visual style variant of the toggle group",
                "enum": ["default"]
              },
              "size": {
                "type": "string",
                "description": "Size of the toggle group items",
                "enum": ["default"]
              },
              "className": {
                "type": "string",
                "description": "Additional CSS classes to apply to the toggle group"
              },
              "children": {
                "type": "any",
                "description": "Child components, typically ToggleGroupItem components"
              }
            }
          },
          "visualLayout": "A horizontally aligned group of toggle items with consistent spacing (gap-1) between them. Items are centered both horizontally and vertically.",
          "examples": [
            "<ToggleGroup type=\"single\">\n  <ToggleGroupItem value=\"a\">A</ToggleGroupItem>\n  <ToggleGroupItem value=\"b\">B</ToggleGroupItem>\n  <ToggleGroupItem value=\"c\">C</ToggleGroupItem>\n</ToggleGroup>"
