      "metadata": {
        "definition": {
          "name": "Dialog",
          "description": "A modal dialog component that renders its children in a floating panel overlaid on the page content. Based on Radix UI's Dialog primitive.",
          "subtype": "reusable",
          "rendersOn": "client",
          "properties": {
            "type": "object",
            "properties": {
              "children": {
                "type": "any",
                "description": "The content to be rendered inside the dialog"
              },
              "open": {
                "type": "boolean",
                "description": "Controls the open state of the dialog"
              },
              "onOpenChange": {
                "type": "function",
                "description": "Event handler called when the open state changes"
              }
            }
          },
          "examples": [
            "<Dialog>\n  <DialogTrigger>Open</DialogTrigger>\n  <DialogContent>\n    <DialogHeader>\n      <DialogTitle>Dialog Title</DialogTitle>\n      <DialogDescription>Dialog Description</DialogDescription>\n    </DialogHeader>\n  </DialogContent>\n</Dialog>"
