          "dependsOn": ["Root"]
        }
      ],
      "metadata": {
        "definition": {
          "name": "Sheet",
          "description": "A dialog component that slides in from the edge of the screen. The root component that wraps the entire sheet functionality.",
          "subtype": "reusable",
          "rendersOn": "client",
          "properties": {
            "type": "object",
            "properties": {
              "children": {
                "type": "any",
                "description": "The content of the sheet"
              },
              "open": {
                "type": "boolean",
                "description": "Controls the open state of the sheet"
              },
              "onOpenChange": {
                "type": "function",
                "description": "Callback function called when the open state changes"
              }
            }
          },
          "examples": [
            "<Sheet>\n  <SheetTrigger>Open Sheet</SheetTrigger>\n  <SheetContent>\n    <SheetHeader>\n      <SheetTitle>Sheet Title</SheetTitle>\n      <SheetDescription>Sheet Description</SheetDescription>\n    </SheetHeader>\n  </SheetContent>\n</Sheet>"
          ]
        },
        "type": "component"
      }
    },
